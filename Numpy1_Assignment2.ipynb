    "import numpy as np\n",
    "\n",
    "def moving_average(intArray, window=3):\n",
    "    weights = np.ones(window) / window\n",
    "    return np.convolve(intArray, weights, mode='valid')\n",
    "\n",
    "data = [3, 5, 7, 2, 8, 10, 11, 65, 72, 81, 99, 100, 150]\n",
    "\n",
    "ma = moving_average(np.asarray(data), 3)\n",
    "print(ma)"
